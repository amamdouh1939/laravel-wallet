{
    "name": "amamdouh1939/laravel-wallet",
    "description": "Easy to use virtual wallet for your app",
    "keywords": [
        "depsimon",
        "laravel-wallet",
        "virtual",
        "currency",
        "credits",
        "wallet"
    ],
    "homepage": "https://github.com/amamdouh1939/laravel-wallet",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Mamdouh",
            "email": "ahmad.mohammed.1939@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/amamdouh1939/laravel-wallet"
        }
    ],
    "autoload": {
        "psr-4": {
            "Depsimon\\Wallet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Depsimon\\Wallet\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Depsimon\\Wallet\\WalletServiceProvider"
            ],
            "aliases": {
                "Wallet": "Depsimon\\Wallet\\WalletFacade"
            }
        }
    }
}
